@model IEnumerable<ResWebsite.DAL.Entity.ExportImportType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h3 class="page-header">Loại nhập xuất</h3>
    </div>
</div>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item " aria-current="page">Trang chủ</li>
        <li class="breadcrumb-item " aria-current="page">Kho</li>
        <li class="breadcrumb-item active" aria-current="page">Loại nhập xuất</li>
    </ol>
</nav>
<div class="row">
    <div class="col-lg-12">
        <p>
            <button class="btn btn-default"><a href="/Admin/ExportImportTypes/Create/" style="text-decoration:none;">Tạo mới loại</a></button>
        </p>
        <table class="table">
            <tr>
                <th>
                    Tên loại
                </th>
                <th>
                    Mô tả
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.TypeName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Description)
                    </td>
                    <td>
                        <button class="btn btn-default"><a href="/Admin/ExportImportTypes/Edit/@item.ExportImportTypeId" style="text-decoration:none;">Sửa</a></button>
                        <button class="btn btn-default"><a href="/Admin/ExportImportTypes/Details/@item.ExportImportTypeId" style="text-decoration:none;">Xem</a></button>
                        <button class="btn btn-default"><a href="/Admin/ExportImportTypes/Delete/@item.ExportImportTypeId" style="text-decoration:none;">Xóa</a></button>
                    </td>
                </tr>
            }

        </table>
    </div>
</div>


